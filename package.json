{
  "name": "awesomestorybook",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "license": "MIT",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --dev-client --web",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories --config-path .ondevice",
    "storybook": "expo start --dev-client",
    "storybook:ios": "expo start --dev-client --ios",
    "storybook:android": "expo start --dev-client --android",
    "snapshot-storybook": "tsx storybook-testing.ts",
    "wait-to-snapshot": "wait-on tcp:localhost:7007 && npm run snapshot-storybook",
    "snapshot-ios": "concurrently --kill-others \"npm run storybook:ios\" \"npm run wait-to-snapshot\"",
    "prebuild": "expo prebuild",
    "start-server": "tsx server.ts",
    "build:screenshot:ios:local": "eas build --profile screenshot --platform ios --local",
    "build:screenshot:android:local": "eas build --profile screenshot --platform android --local",
    "format": "prettier --write .",
    "export": "expo export:embed --entry-file=node_modules/expo/AppEntry.js --bundle-output=main.jsbundle"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.0.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/slider": "4.5.2",
    "expo": "^51.0.38",
    "expo-constants": "~16.0.2",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "expo-dev-client": "~4.0.28"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@expo/metro-runtime": "~3.2.3",
    "@storybook/addon-actions": "^8.3.6",
    "@storybook/addon-controls": "^8.3.6",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-ondevice-actions": "^8.3.9",
    "@storybook/addon-ondevice-backgrounds": "^8.3.9",
    "@storybook/addon-ondevice-controls": "^8.3.9",
    "@storybook/addon-react-native-web": "^0.0.26",
    "@storybook/core": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-native": "^8.3.9",
    "@storybook/react-webpack5": "^8.3.6",
    "@types/react": "~18.2.45",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "babel-plugin-react-native-web": "^0.19.10",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "looks-same": "^9.0.1",
    "magicast": "^0.3.4",
    "storybook": "^8.3.6",
    "tsx": "^4.19.1",
    "typescript": "~5.3.3",
    "wait-on": "^8.0.1",
    "websocket-polyfill": "^0.0.3",
    "prettier": "^3.3.3"
  },
  "resolutions": {
    "react-docgen-typescript": "2.2.2"
  },
  "overrides": {
    "react-docgen-typescript": "2.2.2"
  },
  "private": true
}
